<%@ Page Language="C#" AutoEventWireup="true" MasterPageFile="~/Admin/MasterPage.Master" CodeBehind="ProductBrand.aspx.cs" Inherits="XueFuShop.Admin.ProductBrand" %>
<asp:Content ID="MasterContent" ContentPlaceHolderID="ContentPlaceHolder" runat="server"><div class="position"><img src="Style/Images/PositionIcon.png"  alt=""/>品牌列表</div>	<table class="listTable" cellpadding="0" cellpadding="0">    <tr class="listTableHead">	    <td style="width:5%">ID</td>	    <td style="width:40%; text-align:left;text-indent:8px;">品牌名称</td>	    <td style="width:25%">Url</td> 	    <td style="width:10%">是否推荐</td> 	    <td style="width:15%">管理</td>       	    <td style="width:5%">选择</td>            </tr><asp:Repeater ID="RecordList" runat="server">	<ItemTemplate>	             	<tr class="listTableMain" onmousemove="changeColor(this,'#FFFDD7')" onmouseout="changeColor(this,'#FFF')">			    <td style="width:5%"><%# Eval("ID") %></td>			    <td style="width:40%; text-align:left;text-indent:8px;"><%# Eval("Name") %></td>			    <td style="width:25%"><%# Eval("Url") %></td> 			    <td style="width:10%"><%#XueFuShop.Common.ShopCommon.GetBoolString(Eval("IsTop"))%></td> 			    <td style="width:15%;">			        <a href="javascript:pop('ProductBrandAdd.aspx?ID=<%# Eval("ID") %>',800,450,'课程品牌修改','ProductBrandAdd<%# Eval("ID") %>')"><img src="Style/Images/edit.gif" alt="修改" title="修改" /></a> 			    </td>			    <td style="width:5%"><input type="checkbox" name="SelectID" value="<%# Eval("ID") %>" /></td> 	        		    </tr>        </ItemTemplate></asp:Repeater></table><div class="action">        <input type="button"  value=" 添 加 " class="button"  onclick="pop('ProductBrandAdd.aspx',800,450,'课程品牌添加','ProductClassAdd')"/>&nbsp;<asp:Button CssClass="button" ID="DeleteButton" Text=" 删 除 " OnClientClick="return checkSelect()" runat="server"  OnClick="DeleteButton_Click"/>&nbsp;<input type="checkbox" name="All" onclick="selectAll(this)" />全选/取消</div></asp:Content>
