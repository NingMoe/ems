<%@ Page Language="C#" MasterPageFile="~/Admin/MasterPage.Master" AutoEventWireup="true" CodeBehind="UserXls.aspx.cs" Inherits="XueFuShop.Admin.UserXls" Title="无标题页" %>
<%@ Import Namespace="XueFu.EntLib" %><%@ Import Namespace="XueFuShop.BLL" %><asp:Content ID="MasterContent" ContentPlaceHolderID="ContentPlaceHolder" runat="server">
<div class="position"><img src="/Admin/Style/Images/PositionIcon.png"  alt=""/>用户导入</div><div class="add">		<ul>		<li class="left">公司名称：</li>		<li class="right"><asp:Label ID="CompanyName" runat="server" Text="Label"></asp:Label></li>	</ul>	<ul>		<li class="left">用户名前缀：</li>		<li class="right"><asp:TextBox ID="Prefix" CssClass="input" runat="server"></asp:TextBox></li>	</ul>	<ul id="filein" runat="server"><li class="left">文件路径：</li>		<li class="right"><asp:FileUpload ID="FilePath" CssClass="input" runat="server" Width="300px" /></li>
    </ul></div><div  id="ListShow" runat="server">    <table class="listTable" cellpadding="0">        <tr class="listTableHead">            <td>序号</td>	        <td>部门</td>	        <td>原始岗位</td>	        <td>姓名</td>	        <td>性别</td>	        <td>手机</td>	        <td>邮箱</td>	        <td>账号类型</td>	        <td>学习岗位</td>	        <td>用户名</td>                     </tr>    <asp:Repeater ID="RecordList" runat="server">	    <ItemTemplate>	             	    <tr class="listTableMain" onmousemove="changeColor(this,'#FFFDD7')" onmouseout="changeColor(this,'#FFF')">			        <td><%# Container.ItemIndex + 1%></td>			        <td><%# PostBLL.ReadPost(int.Parse(Eval("Department").ToString())).PostName %></td>			        <td><%# Eval("PostName") %></td>			        <td><%# Eval("RealName") %></td>	                <td><%# EnumHelper.ReadEnumChineseName<SexType>(int.Parse(Eval("Sex").ToString()))%></td>			        <td><%# Eval("Mobile") %></td>			        <td><%# Eval("Email") %></td>			        <td><%# AdminGroupBLL.ReadAdminGroupCache(int.Parse(Eval("GroupId").ToString())).Name %></td>	                <td><%# PostBLL.ReadPost(int.Parse(Eval("StudyPostId").ToString())).PostName%></td>	                <td><%# Eval("UserName") %></td>		        </tr>            </ItemTemplate>    </asp:Repeater>    </table></div><div class="action">    <asp:Button CssClass="button" ID="FileInButton" Text="验证名单" runat="server"  OnClick="FileInButton_Click" /><%--    <asp:Button CssClass="button" ID="FileOutButton" Text="完整试题" runat="server"  OnClick="FileOutButton_Click" />    <asp:Button CssClass="button" ID="QuestionOutButton" Text="题目答案" runat="server" OnClick="QuestionOutButton_Click"/>--%>    <asp:Button CssClass="button" ID="UserOk" Text="名单入库" runat="server" OnClick="UserOk_Click" Visible="False" /></div>
</asp:Content>
